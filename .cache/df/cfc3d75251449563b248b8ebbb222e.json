p.js","original":{"line":40,"column":43}},{"generated":{"line":115,"column":0},"source":"components/App.js","original":{"line":40,"column":16}},{"generated":{"line":115,"column":8},"source":"components/App.js","original":{"line":36,"column":11}},{"generated":{"line":115,"column":9},"source":"components/App.js","original":{"line":35,"column":8}},{"generated":{"line":116,"column":0},"source":"components/App.js","original":{"line":44,"column":5}},{"name":"Component","generated":{"line":120,"column":2},"source":"components/App.js","original":{"line":8,"column":18}},{"name":"App","generated":{"line":122,"column":15},"source":"components/App.js","original":{"line":48,"column":15}}],"sources":{"components/App.js":"import React, { Component } from 'react';\r\nimport Search from './Search';\r\nimport Artist from './Artist';\r\nimport Tracks from './Tracks';\r\n\r\nconst API_ADDRESS = 'https://spotify-api-wrapper.appspot.com'\r\n\r\nclass App extends Component {\r\n    state = {artist: null, tracks: []};\r\n\r\n    componentDidMount(){\r\n        this.searchArtist('Maher Zain');\r\n    }\r\n\r\n    searchArtist = artistQuery =>{\r\n        fetch(`${API_ADDRESS}/artist/${artistQuery}`)\r\n        .then(response => response.json())\r\n        .then(json =>{\r\n            if(json.artists.total>0){\r\n                const artist = json.artists.items[0];\r\n                this.setState({artist});\r\n                fetch(`${API_ADDRESS}/artist/${artist.id}/top-tracks`)\r\n                .then(response => response.json())\r\n                .then(json => this.setState({tracks: json.tracks}))\r\n                .catch(error => alert(error.message))\r\n            }else{\r\n                alert('No artist found!')\r\n            }\r\n        })\r\n        .catch(error => alert(error.message));\r\n    }\r\n   \r\n    render(){\r\n        console.log('this.state',this.state);\r\n        return(\r\n           <div>\r\n                <h2 style={{margin:20}}>Music Master</h2>\r\n                <Search searchArtist={this.searchArtist}/>\r\n                <Artist artist={this.state.artist} />\r\n                <Tracks tracks={this.state.tracks}/>\r\n           </div>\r\n           \r\n        )\r\n    }\r\n}\r\n\r\n\r\nexport default App;"},"lineCount":null}},"error":null,"hash":"fe3c8439a079fe25ea0dcf82b66bfcab","cacheData":{"env":{}}}